{
  "name": "synus",
  "version": "1.0.0",
  "description": "Systematically Yielded, Natively Unobstrusive Sidekick",
  "main": "synus.js",
  "scripts": {
    "test": "npm run lint && npm run docs:test && npm run lint:typings",
    "docs": "docgen --source src --custom docs/index.yml --output docs/docs.json",
    "docs:test": "docgen --source src --custom docs/index.yml",
    "lint": "eslint src *.js",
    "lint:fix": "eslint --fix src",
    "lint:typings": "tslint typings/index.d.ts",
    "build:browser": "webpack",
    "prepublishOnly": "npm run test && NODE_ENV=production npm run build:browser"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mladenbrankovic/synus.git"
  },
  "keywords": [
    "synus",
    "discord.js",
    "discord",
    "bot",
    "js",
    "javascript"
  ],
  "author": "@mladenbrankovic",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mladenbrankovic/synus/issues"
  },
  "homepage": "https://github.com/mladenbrankovic/synus#readme",
  "dependencies": {
    "chalk": "^2.4.2",
    "discord.js": "^11.4.2",
    "dotenv": "^7.0.0",
    "moment": "^2.24.0",
    "moment-duration-format": "^2.2.2",
    "node-fetch": "^2.3.0",
    "npm": "^6.9.0"
  }
}